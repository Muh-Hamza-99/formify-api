generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id BigInt @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
}